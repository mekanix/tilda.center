  "version": "0.0.1",
  "private": false,
  "dependencies": {
    "freenit": "^0.0.15",
    "prop-types": "^15.7.2",
    "radium": "^0.26.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "react-swipeable-views": "^0.13.9",
    "react-swipeable-views-utils": "^0.14.0-alpha.0",
    "resumablejs": "^1.1.0",
    "strophe.js": "^1.4.0"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
